"Vundle
set nocompatible
filetype off
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'

call vundle#end()
filetype plugin indent on
"VundleEnd

syntax on
set number relativenumber
set splitbelow
set splitright
colo desert
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab

